sis() {\n      let myChart = this.$echarts.init(\n        document.getElementById(\"topic_analysis_graph\")\n      );\n      let option;\n      let query = this.$route.query;\n      this.$axios\n        .get(\n            \"comment/cluster/type?tag_task_id=\"+query.tag_task_id+\"&weibo_id=\"+query.weibo_id\n        )\n        .then((res) => {\n          console.log(res)\n          for (let index in res.data.data) {\n            this.topic_analysis[index] = res.data.data[index];\n            this.topic_analysis[index].name = res.data.data[index].key;\n            this.topic_analysis[index].value = res.data.data[index].doc_count;\n          }\n          option = {\n            legend: {\n              orient: \"vertical\",\n              right: \"right\",\n            },\n            tooltip: {\n              trigger: \"item\",\n            },\n            series: [\n              {\n                name: \"主题分析\",\n                type: \"pie\",\n                radius: \"50%\",\n                data: this.topic_analysis,\n                emphasis: {\n                  itemStyle: {\n                    shadowBlur: 10,\n                    shadowOffsetX: 0,\n                    shadowColor: \"rgba(0, 0, 0, 0.5)\",\n                  },\n                },\n              },\n            ],\n          };\n          myChart.setOption(option);\n        });\n      option && myChart.setOption(option);\n    },\n  },\n  mounted() {\n    this.myTopicAnalysis();\n  },\n};\n</script>\n\n<style scoped>\n.topic_analysis {\n  position: absolute;\n  width: 100%;\n  height: 25%;\n  top: 20%;\n  background-color: #fff;\n}\n.topic_analysis_title {\n  margin: 10px 20px;\n  padding: 5px;\n  font-weight: 600;\n  letter-spacing: 1px;\n}\n#topic_analysis_graph {\n  width: 450px;\n  height: 220px;\n  position: relative;\n  top: -45px;\n  left: -20px;\n}\n</style>"]}]}